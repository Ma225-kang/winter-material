
<div class="form-new-equipment">
  <h1>Modify your personnal data</h1>
  <%= simple_form_for @user do |f|%>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit 'modify' , class: "btn btn-primary" %>
  <% end %>

